{
  "redirects": [
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "minetrack.site"
        }
      ],
      "destination": "https://www.minetrack.site",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "minetrack.site"
        }
      ],
      "destination": "https://www.minetrack.site/:path*",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ],
  "trailingSlash": false
}
