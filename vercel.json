{
  "redirects": [
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "minetrack.site"
        }
      ],
      "destination": "https://www.minetrack.site",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "minetrack.site"
        }
      ],
      "destination": "https://www.minetrack.site/:path*",
      "permanent": true
    }
  ],
  "trailingSlash": false
}
